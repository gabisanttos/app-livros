{
	"name": "backend-capitu",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon --exec ts-node ./src/index.ts",
		"migration:generate": "npx ts-node ./node_modules/typeorm/cli.js migration:generate",
		"migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
		"migration:revert": "npx ts-node ./node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
		"build": "rm -rf ./dist && tsc",
		"start": "node ./dist/index.js"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.13",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.0.1",
		"nodemon": "^2.0.19",
		"ts-node": "^10.8.2",
		"typeorm-ts-node-commonjs": "^0.3.20",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"axios": "^1.12.2",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-async-errors": "^3.1.1",
		"groq-sdk": "^0.32.0",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.7.3",
		"pg-connection-string": "^2.9.1",
		"postgres": "^3.4.7",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.27"
	}
}
